# Anthropic API — required for the chat assistant feature
ANTHROPIC_API_KEY=sk-ant-your-key-here

# OpenAI — fallback model if Anthropic API is unavailable
OPENAI_API_KEY=sk-your-openai-key-here

# Google AdSense — add your publisher ID after AdSense approval
NEXT_PUBLIC_ADSENSE_PUBLISHER_ID=ca-pub-your-id-here

# Site URL — must be set to production domain for OG images and canonical URLs
NEXT_PUBLIC_SITE_URL=https://timemeaning.com

# IANA database version — update when incorporating new releases
NEXT_PUBLIC_IANA_VERSION=2026a
